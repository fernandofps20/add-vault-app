<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:form="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
		<form:SimpleForm>
			<Label text="{i18n>secretType}" />
			<Text text="{retrieveSecret>/data/secretType}" />
			<Label text="{i18n>secretAlias}" />
			<Text text="{retrieveSecret>/data/secretAlias}" />
			<Label text="{i18n>tenant}" />
			<Text text="{retrieveSecret>/data/tenant}" />
			<Label text="{i18n>clientId}" />
			<Text text="{retrieveSecret>/data/clientId}" />
			<Label text="{i18n>clientSecret}" />
			<Text text="{retrieveSecret>/data/clientSecret}" />
			<Label text="{i18n>secretExpiryDate}" />
			<Text text="{retrieveSecret>/data/secretExpiryDate}" />
			<Label text="{i18n>updatedAt}" />
			<Text text="{retrieveSecret>/data/updatedAt}" />
        </form:SimpleForm>
</core:FragmentDefinition>