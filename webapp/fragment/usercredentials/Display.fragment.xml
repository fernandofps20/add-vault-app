<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:form="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
		<form:SimpleForm>
			<Label text="secretType" />
			<Text text="{retrieveSecret>/data/secretType}" />
			<Label text="secretAlias" />
			<Text text="{retrieveSecret>/data/secretAlias}" />
			<Label text="userDomain" />
			<Text text="{retrieveSecret>/data/userDomain}" />
			<Label text="userAccount" />
			<Text text="{retrieveSecret>/data/userAccount}" />
			<Label text="userPassword" />
			<Text text="{retrieveSecret>/data/userPassword}" />
			<Label text="passwordExpiryDate" />
			<Text text="{retrieveSecret>/data/passwordExpiryDate}" />
			<Label text="userDirectoryAddress" />
			<Text text="{retrieveSecret>/data/userDirectoryAddress}" />
			<Label text="updatedAt" />
			<Text text="{retrieveSecret>/data/updatedAt}" />
        </form:SimpleForm>
</core:FragmentDefinition>