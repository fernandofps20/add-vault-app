<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:form="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
		<form:SimpleForm>
			<Label text="secretType" />
			<Text text="{retrieveSecret>/data/secretType}" />
			<Label text="secretAlias" />
			<Text text="{retrieveSecret>/data/secretAlias}" />
			<Label text="mailId" />
			<Text text="{retrieveSecret>/data/mailId}" />
			<Label text="mailAddress" />
			<Text text="{retrieveSecret>/data/mailAddress}" />
			<Label text="mailUserDomain" />
			<Text text="{retrieveSecret>/data/mailUserDomain}" />
			<Label text="mailUserAccount" />
			<Text text="{retrieveSecret>/data/mailUserAccount}" />
			<Label text="mailPassword" />
			<Text text="{retrieveSecret>/data/mailPassword}" />
			<Label text="passwordExpiryDate" />
			<Text text="{retrieveSecret>/data/passwordExpiryDate}" />
			<Label text="updatedAt" />
			<Text text="{retrieveSecret>/data/updatedAt}" />
        </form:SimpleForm>
</core:FragmentDefinition>